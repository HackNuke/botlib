#!/usr/bin/env python3
# This file is placed in the Public Domain.


"bot command"


import sys


from bot.handler import Handler
from bot.kernel import Cfg, kcmd


import bot.all


Cfg.wd = "/var/lib/botd/"


class CLI(Handler):

    def raw(self, txt):
        print(txt)


def main():
    c = CLI()
    kcmd(c, " ".join(sys.argv[1:]))


main()
