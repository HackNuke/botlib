grep -R __import__ bot
grep -R __main__ bot
grep -R __version__ bot
grep -R "## " bot
grep -R popen bot
grep -R pkgutil bot
grep -R importlib bot
grep -R print bot
grep -R "import sys" bot
grep -R "import inspect" bot


