apt purge update-notifier-common -y
apt purge unattended-upgrades -y
apt purge sysstat -y
apt purge gnome-initial-setup -y 
apt-add-repository contrib
apt-add-repository non-free
apt update
apt install -y irssi ircd-irc2 python3-setuptools python3-pip python3-virtualenv tilix twine pylint
apt install -y audacious thunderbird
apt install -y firmware-linux-nonfree
apt install -y nvidia-driver
apt clean -y
apt autoremove
apt-add-repository -r contrib
apt-add-repository -r non-free
apt update