#!/usr/bin/python3
"library for programming objects"

import os, sys ; sys.path.insert(0, "lib")
import obj

from csl import Console
from prs import parse_cli
from trm import execute, os

def main():
    obj.wd = os.path.expanduser("~/.bot")
    cfg = parse_cli()
    c = Console()
    c.walk("cmds,bot")
    if cfg.txt:
       return c.cmd(cfg.txt)
    c.init(cfg.mods)
    c.start()
    c.wait()
     
execute(main)
os._exit(0)
