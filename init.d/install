[ ! -d "/var/log/botlib" ] && mkdir /var/log/botlib
[ ! -d "/var/lib/botlib" ] && mkdir /var/lib/botlib
[ ! -d "/var/lib/botlib/store" ] && mkdir /var/lib/botlib/store
[ ! -d "/var/lib/botlib/store/bl.krn.Cfg" ] && mkdir /var/lib/botlib/store/bl.krn.Cfg
[ ! -d "/var/lib/botlib/store/bl.krn.Cfg/2019-12-18" ] && mkdir /var/lib/botlib/store/bl.krn.Cfg/2019-12-18
[ ! -d "/var/lib/botlib/store/bl.irc.Cfg" ] && mkdir /var/lib/botlib/store/bl.irc.Cfg
[ ! -d "/var/lib/botlib/store/bl.irc.Cfg/2019-12-18" ] && mkdir /var/lib/botlib/store/bl.irc.Cfg/2019-12-18
cp init.d/botlib.service /etc/systemd/system
cp init.d/default /var/lib/botlib/store/bl.krn.Cfg/2019-12-18/15\:54\:13.868840
cp init.d/irc.cfg /var/lib/botlib/store/bl.irc.Cfg/2019-12-18/15\:54\:13.868840
#bin/bl -x meet ~bart@127.0.0.1 -d /var/lib/botlib -m usr
systemctl enable botlib
systemctl daemon-reload
service botlib start
systemctl status botlib
